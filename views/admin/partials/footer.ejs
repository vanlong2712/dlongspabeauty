<!-- Initialize the editor. -->
<script>
    $(function() {
        $('textarea').froalaEditor({
            height: 300
        })
    });

    function accept_enable(msg) {
        if(window.confirm(msg)){
            return true;
        }
        return false;
    }

    $(document).ready(function(){
        $("div.switch").on("click", function(){
            var input = $(this).find("input");
            var nameToggle = input.attr("name");
            if(nameToggle !== 'ToggleInList') {
              return false;
            }
            var msg = '';
            id = input.attr("serviceId");
            var boolean = '';
            if(!input.is(':checked')) {
                msg = 'Dịch vụ "'+ input.attr("serviceName") +'" ngưng hoạt động';
                boolean = false;
            }else {
                msg = 'Dịch vụ "'+ input.attr("serviceName") +'" Hoạt động';
                boolean = true;
            }
            $.get("./service-enable/" + id + "/" + boolean, (data) => {
                console.log(data);
            }).then(() => toastr.success(msg));
        });

        $(document).on("click","#service-delete", function(){
            var id = $(this).attr("serviceId");
            var name = $(this).attr("serviceName")
            msg = 'Bạn muốn xóa dịch vụ "'+ name +'"?';

            if(window.confirm(msg)){

                $.get("./service-delete/" + id, function(data) {
                    toastr.success(data);
                    setTimeout(function(){
                        window.location.href="./service-list";
                    }, 1500);
                });


            }
            return false;
        });

        $(document).on("click","#service-edit", function(){
            var id = $(this).attr("serviceId");
            location.href="./service-edit/"+id;
        });
    });
</script>

